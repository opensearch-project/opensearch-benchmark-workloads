{
  "articles": {
    "dynamic": "strict",
    "_all": {
      "enabled": false
    },
    "properties": {
      "name": {
        "type": "keyword"
      },
      "journal": {
        "type": "text"
      },
      "date": {
        "type": "text"
      },
      "volume": {
        "type": "text"
      },
      "issue": {
        "type": "text"
      },
      "accession": {
        "type": "keyword"
      },
      "timestamp": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss"
      },
      "pmid": {
        "type": "integer"
      },
      "body": {
        "type": "text"
      }
    }
  }
}