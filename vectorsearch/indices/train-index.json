{
    "settings": {
      "index": {
        "number_of_shards": {{ train_index_primary_shards }}
        {%- if target_index_replica_shards is defined %}
        ,"number_of_replicas": {{ train_index_replica_shards }}
        {%- endif %}
      }
    },
    "mappings": {
      "properties": {
        "{{ train_field_name }}": {
          "type": "knn_vector",
          {%- if target_index_dimension is defined and target_index_dimension %}
            "dimension": {{ target_index_dimension }}
          {%- endif %}
        }
      }
    }
  }
  