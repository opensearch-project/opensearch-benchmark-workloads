        {% else %}
          {
            "source-file": "documents-2.json.bz2",
            "document-count": 11396503,
            "compressed-bytes": 265208777,
            "uncompressed-bytes": 3547613828
          }
        {% endif %}
      ]
    }
  ],
  "operations": [
    {{ benchmark.collect(parts="operations/*.json") }}
  ],
  "test_procedures": [
    {{ benchmark.collect(parts="test_procedures/*.json") }}
  ]
